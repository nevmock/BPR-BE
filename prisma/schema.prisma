generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String      @id @default(uuid())
  username  String @unique
  password  String
  KSSM      KSSM[]
  KSS       KSS[]
  PINEK     PINEK[]
  FLEKSI    FLEKSI[]
  PROCIM    PROCIM[]
  KSM       KSM[]
  KMSM      KMSM[]
  KRS       KRS[]
  KMM       KMM[]
  KMS       KMS[]
  KEF       KEF[]
  KAR       KAR[]
  role String @default("PIC")

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  system_logs SystemLog[]

  @@map("users")
}

